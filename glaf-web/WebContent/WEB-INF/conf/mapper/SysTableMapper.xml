	<select id="getTableColumns" parameterType="com.glaf.matrix.data.query.SysTableQuery"
		resultType="com.glaf.matrix.data.domain.SysTable">

		select C.TABLENAME_ tableName, count(C.COLUMNNAME_) as columnQty
		from
		SYS_EXT_COLUMN C
		inner join SYS_EXT_TABLE E
		on C.TABLENAME_ =
		E.TABLENAME_

		<include refid="selectTableCountSql" />

		group by C.TABLENAME_
		order by C.TABLENAME_

	</select>


